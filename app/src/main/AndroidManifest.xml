<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.mamunsproject.youtubekids">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <queries>
        <intent>
            <action android:name="com.google.android.youtube.api.service.START" />
        </intent>
    </queries>

    <application
        android:name=".OneSignalClass"
        android:allowBackup="true"
        android:icon="@drawable/iconof_cartoon_tube"
        android:label="@string/app_name"
        android:roundIcon="@drawable/iconof_cartoon_tube"
        android:supportsRtl="true"
        android:theme="@style/Theme.YoutubeKids"
        tools:replace="android:icon">
        <activity
            android:name=".VideoDetailedActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.Hindi_Cartoon"
            android:exported="false" />
        <activity
            android:name=".Activity.MrBeanCartoon"
            android:exported="false" />
        <activity
            android:name=".Activity.BanglaCartoon"
            android:exported="false" />
        <activity
            android:name=".Activity.Oggy_and_coakroaches"
            android:exported="false" />
        <activity
            android:name=".Activity.Ben10"
            android:exported="false" />
        <activity
            android:name=".Activity.Doraemon"
            android:exported="false" />
        <activity
            android:name=".Activity.MotuPatlu"
            android:exported="false" />
        <activity
            android:name=".Activity.TomJerry"
            android:exported="false" />
        <activity
            android:name=".PlayerActivity"
            android:configChanges="orientation|keyboardHidden|screenSize|layoutDirection"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Games.No_internet"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Games.Mind"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Games.Cards"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Games.Classic"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Games.Racing"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Games.Action"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Games.Games_MainActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".Games.GameLoader" />
        <activity
            android:name=".Games.Contact"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Games.Category"
            android:screenOrientation="portrait" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>